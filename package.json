{
  "name": "langcrag-capec",
  "version": "1.0.0",
  "description": "Sistema RAG para análise de ataques CAPEC usando TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "create-db": "echo 'INFO: Certifique-se de que o servidor Ollama está em execução. Use \"npm run ollama:start\" em outro terminal, se necessário.' && ts-node src/createDatabase.ts",
    "update-db": "ts-node src/updateDatabase.ts",
    "ollama:start": "ollama serve",
    "chat": "ts-node src/chat.ts",
    "test-ollama": "ts-node src/testOllama.ts",
    "web": "ts-node src/server.ts"
  },
  "keywords": [
    "rag",
    "capec",
    "security",
    "typescript",
    "langchain"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.0.20",
    "@langchain/openai": "^0.0.14",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "langchain": "^0.1.0",
    "mammoth": "^1.7.2",
    "ollama": "^0.5.0",
    "pdf-parse": "^1.1.1",
    "typescript": "^5.2.2",
    "vectordb": "^0.1.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/xml2js": "^0.4.14",
    "ts-node": "^10.9.1"
  }
}
